{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Database" : "data source = database.db"
  },
  "Stripe" : {
    "SecretKey" : "",
    "PublishableKey" : ""
  },
  "WebsiteUrl" : "http://localhost:5173",
  "SmtpClient" : {
    "Host" : "",
    "Port" : 0,
    "Email" : "",
    "Password" : ""
  },
  "GoogleApiKey" : "",
  "Jwt": {
    "Key": ""
  },
  "Serilog" : {
    "Using" : [ "Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "WriteTo" : [
      { "Name" :  "Console" },
      { "Name" :  "File",
        "Args": {
          "path" : "logs/log-.txt",
          "rollingInterval" : "Day"
        }}
    ]
  }
}
